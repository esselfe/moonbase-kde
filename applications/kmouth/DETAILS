          MODULE=kmouth
         VERSION=25.08.1
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:866ddd1f1936ee2df7b90817e90527655d570d80cdc04d6cd3918e234c9c3857
