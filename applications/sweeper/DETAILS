          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/release-service/$VERSION/src
      SOURCE_VFY=sha256:e0a092c3f834715087a38927c3c27f4e1cffc5962404d8e3fef69c3ba6eda3ad
