          MODULE=kdnssd
         VERSION=6.15.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:192164a395ce9f0667184cae78f369a7c0b8acab0e0d073cdedb2a54bc7c4d5a
