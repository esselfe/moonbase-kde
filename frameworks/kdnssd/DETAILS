          MODULE=kdnssd
         VERSION=6.13.0
          SOURCE=$MODULE-$VERSION.tar.xz
      SOURCE_URL=$KDE_URL/stable/frameworks/$(module_version kf5)
      SOURCE_VFY=sha256:e3d25dfaa3886dff4851e5b88f652f6bc6e8e8718708bd80ba49875141907e82
